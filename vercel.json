{
  "version": 2,
  "builds": [{ "src": "index.php", "use": "vercel-php@0.6.0" }],
  "routes": [
    { "src": "/", "dest": "index.php" }，
    { "src": "/account", "dest": "account.php" }，
    { "src": "/footer", "dest": "footer.php" }，
    { "src": "/header", "dest": "header.php" }，
    { "src": "/login", "dest": "login.php" }，
    { "src": "/logout", "dest": "logout.php" }，
    { "src": "/signup", "dest": "signup.php" }
  ]
}
